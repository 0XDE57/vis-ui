<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="pl.kotcrab.vis.VisSceneEditor">
	<source path="sceneeditor">
		<exclude name="module/DesktopHandler.java" />
	</source>

	<extend-configuration-property name="gdx.files.classpath"
		value="pl/kotcrab/vis/arial.fnt" />

	<extend-configuration-property name="gdx.files.classpath"
		value="pl/kotcrab/vis/arial.png" />

	<extend-configuration-property name="gdx.reflect.include"
		value="pl.kotcrab.vis.sceneeditor.serializer.ObjectInfo" />

	<extend-configuration-property name="gdx.reflect.include"
		value="pl.kotcrab.vis.sceneeditor.serializer.ObjectsData" />
</module>